{
    "manifest_version": 3,
    "name": "MES Fuller",
    "description": "Makes your MES tests apear full",
    "version": "0.1",
    "action": {
        "default_popup": "buttons.html"
    },
    "content_scripts": [
        {
            "js": ["build/content.js"],
            "matches": [
                "https://uchebnik.mos.ru/*"
            ]
        }
    ],
    "permissions": [ "activeTab", "tabs" ]
}